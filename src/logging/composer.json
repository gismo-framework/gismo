{
    "name": "gismo/logging",
    "type": "library",
    "description": "Gismo Logging Component",
    "keywords": ["Gismo", "log", "logging"],
    "homepage": "https://getgismo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Leuffen",
            "email": "matthes@leuffen.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "gismo/php-foundation": "*",
        "firephp/firephp-core": "*"
    },
    "autoload": {
        "psr-4": {
            "Gismo\\Component\\Logging\\": "src/Logging",
            "Gismo\\Test\\Component\\Logging\\": "test"
        },
        "exclude-from-classmap": [
            "/test/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
