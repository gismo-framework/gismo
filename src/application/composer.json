{
    "name": "gismo/application",
    "type": "library",
    "description": "Gismo Application Component",
    "keywords": ["Gismo", "application"],
    "homepage": "https://getgismo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Leuffen",
            "email": "matthes@leuffen.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "gismo/php-foundation": "*",
        "gismo/di": "*",
        "gismo/http-foundation": "*",
        "gismo/route": "*"
    },
    "autoload": {
        "psr-4": {
            "Gismo\\Component\\Application\\": "src/Application",
            "Gismo\\Test\\Component\\Application\\": "test"
        },
        "exclude-from-classmap": [
            "/test/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
